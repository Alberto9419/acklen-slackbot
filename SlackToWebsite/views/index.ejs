<html>
  <head>
    <title>SlackToWebSite</title>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.3.10/angular.min.js"></script>
    <script src="http://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.10/angular-ui-router.js"></script>
    <script src="/javascripts/angularApp.js"></script>
  </head>
  <body ng-app="SlackToWebSite" ng-controller="MainCtrl">
    <div>
       <span>Number of Messages</span>
       <input type="text" id="MessagesNumber" ng-model="numberMessages" />
   </div>
   <div>
       <span>Channel Name</span>
       <input type="text" id="ChannelName" ng-model="channelName" />
   </div>
   <button type="button" id="getMessages" ng-click="getChannelMessages()">Get Messages</button>

   <div ng-repeat="message in messages">
     {{message}}
   </div>
  </body>
</html>